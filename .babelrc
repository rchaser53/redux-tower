{
  "env": {
    "development": {
      "sourceMaps": "inline"
    },
    "production": {
      "presets": [
        "react","es2015", "es2016", "stage-2"
      ],
      "plugins": [
        "babel-plugin-unassert",
        "babel-plugin-transform-remove-console"
      ]
    },
    "debug": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "test": {
      "presets": [
        "react","es2015", "es2016", "stage-2"
      ]
    }
  }
}
